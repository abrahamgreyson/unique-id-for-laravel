{
    "name": "abe/unique-id-for-laravel",
    "description": "This is my package string-key-for-laravel",
    "keywords": [
        "laravel",
        "unique-key-for-laravel",
        "primary key",
        "id",
        "uuid",
        "snowflake"
    ],
    "homepage": "https://github.com/abrahamgreyson/string-key-for-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "abraham greyson",
            "email": "82011220@qq.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ramsey/uuid": "^3.0|^4.0",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "godruoyi/php-snowflake": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "UniqueIdForLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UniqueIdForLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "suggest": {
        "ramsey/uuid": "Required to generate uuid (^3.0|^4.0).",
        "godruoyi/php-snowflake": "Required to generate snowflake id (^1.0|^2.0)."
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "abe": {
            "wechat": "abrahamgreyson"
        },
        "laravel": {
            "providers": [
                "UniqueIdForLaravel\\UniqueIdForLaravelServiceProvider"
            ],
            "aliases": {
                "StringKeyForLaravel": "UniqueIdForLaravel\\Facades\\UniqueIdForLaravel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
